{
	"name": "proto-mass",
	"description": "Indirect Control Pixel-Art Svelte-based Sandbox Browser Game",
	"version": "0.0.1",
	"author": {
		"name": "Tkachuk Zakhar",
		"url": "https://proto-mass.netlify.app/",
		"email": "blashirk@gmail.com"
	},
	"license": "MIT",
	"keywords": [
		"sveltekit",
		"svelte based",
		"typescript",
		"svelte",
		"SSR",
		"Phaser",
		"svelte-phaser",
		"pixel art",
		"sandbox",
		"game"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com:imhul/proto-mass-svelte.git"
	},
	"scripts": {
		"prebuild": "rimraf .svelte-kit && rimraf build && rimraf .vercel_build_output",
		"check-env": "node --loader ts-node/esm scripts/js/check-env.ts",
		"dev:sveltekit": "svelte-kit dev",
		"dev:sveltekit:host": "svelte-kit dev --host=0.0.0.0",
		"dev": "npm run check-env && npm run dev:sveltekit",
		"dev:host": "npm run check-env && npm run dev:sveltekit:host",
		"dev:open": "npm run dev --open",
		"dev:host:open": "npm run dev --open --host",
		"build:env:prod": "node --loader ts-node/esm scripts/js/environment.setup.ts --mode=prod --project=proto-mass --arhitect-type=build --configurations=prod",
		"build:env:dev": "node --loader ts-node/esm scripts/js/environment.setup.ts --mode=dev --project=proto-mass --arhitect-type=build --configurations=dev",
		"build:env:qa": "node --loader ts-node/esm scripts/js/environment.setup.ts --mode=qa --project=proto-mass --arhitect-type=build --configurations=qa",
		"build:env:local": "node --loader ts-node/esm scripts/js/environment.setup.ts --mode=local --project=proto-mass --arhitect-type=build --configurations=local",
		"build:sveltekit": "cross-env-shell svelte-kit build",
		"build": "run-s build:env:prod build:sveltekit build:env:local",
		"build:verbose:sveltekit": "cross-env-shell svelte-kit build --verbose",
		"build:verbose": "run-s build:verbose:sveltekit",
		"build:vercel": "cross-env ADAPTER=vercel OPTIONS='{}' npm run build",
		"build:vercel:verbose": "cross-env ADAPTER=vercel OPTIONS='{}' npm run build:verbose",
		"build:vercel:ssr": "cross-env ADAPTER=vercel OPTIONS='{}' SSR=true npm run build",
		"build:vercel:verbose:ssr": "cross-env ADAPTER=vercel OPTIONS='{}' SSR=true npm run build:verbose",
		"preview": "svelte-kit preview",
		"preview:open": "npm run run preview --open",
		"preview:host": "npm run run preview --host",
		"preview:host:open": "npm run run preview --open --host",
		"format": "prettier --ignore-path .gitignore --write ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} --plugin-search-dir=.",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "1.0.0-next.26",
		"@sveltejs/kit": "1.0.0-next.142",
		"@types/node": "^16.7.2",
		"@types/node-fetch": "^2.5.12",
		"@types/nprogress": "^0.2.0",
		"autoprefixer": "^10.3.3",
		"cross-env": "^7.0.3",
		"cssnano": "^5.0.6",
		"cssnano-preset-advanced": "^5.1.4",
		"dotenv": "^10.0.0",
		"node-fetch": "^2.6.1",
		"npm-run-all": "^4.1.5",
		"nprogress": "^0.2.0",
		"phaser": "^3.55.2",
		"postcss": "^8.3.5",
		"postcss-cli": "^8.3.1",
		"postcss-load-config": "^3.1.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"rimraf": "^3.0.2",
		"sass": "^1.52.1",
		"svelte": "^3.48.0",
		"svelte-check": "^2.2.5",
		"svelte-loader": "^3.1.2",
		"svelte-phaser": "^0.1.0-beta.2",
		"svelte-preprocess": "^4.10.6",
		"svelte-simple-modal": "^1.3.3",
		"ts-node": "^10.2.1",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0"
	},
	"type": "module",
	"engines": {
		"node": ">= 14.16.1"
	},
	"dependencies": {
		"uifx": "^2.0.7",
		"uuid": "^8.3.2"
	}
}
